<?xml version="1.0" encoding="UTF-8"?>
<Installer>
	<Name>Natron</Name>
	<Version>_VERSION_</Version>
	<Title>Natron</Title>
	<Publisher>INRIA</Publisher>
	<StartMenuDir>Natron</StartMenuDir>
	<ProductUrl>http://www.natron.fr</ProductUrl>
	<RemoveTargetDir>false</RemoveTargetDir>
	<AllowSpaceInPath>true</AllowSpaceInPath>
	<UninstallerName>NatronSetup</UninstallerName>
	<InstallerWindowIcon>icon.png</InstallerWindowIcon>
	<Logo>logo.png</Logo>
	<Watermark>watermark.png</Watermark>
        <AdminTargetDir>/opt/Natron2</AdminTargetDir>
	<TargetDir>@HomeDir@/Natron2</TargetDir>
	<RemoteRepositories>
		<Repository>
			<Url>_URL_/_OS_BRANCH_BIT_</Url>
			<Enabled>1</Enabled>
                        <DisplayName>Natron _RBVERSION_ Patch Releases</DisplayName>
		</Repository>
                <Repository>
                        <Url>_URL_/_OS_BRANCH_LATEST_</Url>
                        <Enabled>0</Enabled>
                        <DisplayName>Natron Latest Releases</DisplayName>
                </Repository>
	</RemoteRepositories>
</Installer>
